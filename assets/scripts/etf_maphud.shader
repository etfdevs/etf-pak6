textures/etf_hud/red_hecharge_marker
{
	qer_editorimage textures/etf_hud/newbalert_hecharge.tga
	surfaceparm nonsolid
	surfaceparm trans
	nomipmaps
	{
		map textures/etf_hud/newbalert_hecharge.tga
		blendfunc blend
		rgbGen const ( 1 0 0 )
	}
	{
		map textures/etf_hud/newbalert_hecharge_blend2.tga
		blendfunc add
		rgbGen wave sin 0.25 1 0 0.25 
	}
}

